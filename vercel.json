{
  "version": 2,
  "builds": [
    {
      "src": "ProgettoEventi/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12",
        "buildCommand": "bash build.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "ProgettoEventi/wsgi.py"
    }
  ],
  "regions": ["fra1"]
}